{
  "name": "@genesis/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "ci": "yarn run typecheck && yarn run lint && yarn run test --ci --coverage --coverageReporters=text",
    "test": "jest",
    "test:ci": "JEST=true jest --coverage --silent --ci --coverageReporters=text --coverageReporters=cobertura --testResultsProcessor=jest-junit",
