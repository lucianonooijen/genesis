    "lint:fix": "yarn run lint --fix",
    "typecheck": "tsc",
    "aegis": "decoder-generator generate --inputPath ./packages/api/types --outputFile packages/api/internal/decoders.ts && eslint packages/api/internal/decoders.ts --fix"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
